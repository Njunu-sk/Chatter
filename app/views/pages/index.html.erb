 <!-- Masthead-->
        <header class="masthead">
            <div class="container d-flex h-100 align-items-center">
                <div class="mx-auto text-center">
                    <h1 class="mx-auto my-0 text-uppercase">KULTURE MYUZIK</h1>
                    <h2 class="text-white-50 mx-auto mt-2 mb-5">An online music promo application</h2>
                    <% if user_signed_in? %>
                    <%= link_to 'Playlist', songs_path , class: "btn btn-primary js-scroll-trigger" %>
                    <%= link_to 'Events', events_path , class: "btn btn-primary js-scroll-trigger" %>
                    <% else %>
                    <%= link_to 'Log In', new_user_session_path , class: "btn btn-primary js-scroll-trigger" %>
                    <% end %>
                </div>
            </div>
        </header>
        <!-- About-->
        <section class="about-section text-center" id="about">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 mx-auto">
                        <p class="text-white-50">
                        Kulture Myuzik is a one stop shop for entertainment, one can upload, stream and comment on a song or an upload.
                        </p>
                        
                    </div>
                </div>
                
                 
                 <%= image_tag("headphones.jpg", :class => "img-fluid", :alt => "") %>
            </div>
        </section>
        <!-- Projects-->
        <section class="projects-section bg-light" id="projects">
            <div class="container">
                <!-- Featured Project Row-->
                <div class="row align-items-center no-gutters mb-4 mb-lg-5">
                    <div class="col-xl-8 col-lg-7"><%= image_tag("playlist.jpg", :class => "img-fluid", :alt =>"" )%></div>
                    <div class="col-xl-4 col-lg-6">
                    <div class="bg-black text-center h-100 project">
                            <div class="d-flex h-100">
                                <div class="project-text w-100 my-auto text-center text-lg-left">
                                    <h4 class="text-white"><%= link_to "Songs", songs_path %></h4>
                                    <p class="mb-0 text-white-50">One can upload songs to this platform so that other users can be able to view it and stream also download</p>
                                    <hr class="d-none d-lg-block mb-0 ml-0" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Project One Row-->
                <div class="row justify-content-center no-gutters mb-5 mb-lg-0">
               
                    <div class="col-lg-6"> <%= image_tag("love.jpg", :class => "img-fluid", :alt =>"" )%></div>
                    <div class="col-lg-6">
                        <div class="bg-black text-center h-100 project">
                            <div class="d-flex h-100">
                                <div class="project-text w-100 my-auto text-center text-lg-left">
                                    <h4 class="text-white">Comments</h4>
                                    <p class="mb-0 text-white-50">users of the system can comment on the song and have chats about the song</p>
                                    <hr class="d-none d-lg-block mb-0 ml-0" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Project Two Row-->
                <div class="row justify-content-center no-gutters">
                    <div class="col-lg-6"> <%= image_tag("event.jpg", :class => "img-fluid", :alt =>"" )%></div>
                    <div class="col-lg-6 order-lg-first">
                        <div class="bg-black text-center h-100 project">
                            <div class="d-flex h-100">
                                <div class="project-text w-100 my-auto text-center text-lg-right">
                                    <h4 class="text-white"><%= link_to "Events", events_path %></h4>
                                    <p class="mb-0 text-white-50">Users of the system can create events and display them to the platform for other users to view</p>
                                    <hr class="d-none d-lg-block mb-0 mr-0" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- Signup-->
        <section class="signup-section" id="signup">
            <div class="container">
                <div class="row">
                    <div class="col-md-10 col-lg-8 mx-auto text-center">
                        <i class="far fa-paper-plane fa-2x mb-2 text-white"></i>
                        <h2 class="text-white mb-5">Subscribe to receive updates!</h2>
                        <form class="form-inline d-flex">
                            <input class="form-control flex-fill mr-0 mr-sm-2 mb-3 mb-sm-0" id="inputEmail" type="email" placeholder="Enter email address..." />
                            <button class="btn btn-primary mx-auto" type="submit">Subscribe</button>
                        </form>
                    </div>
                </div>
            </div>
        </section>
